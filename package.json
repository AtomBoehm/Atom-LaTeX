{
  "name": "atom-latex",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "The LaTeX package you need.",
  "license": "MIT",
  "keywords": [
    "tex",
    "latex",
    "compile",
    "preview"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-latex:build",
      "atom-latex:preview",
      "atom-latex:kill",
      "atom-latex:show-log"
    ]
  },
  "activationHooks": [
    "language-latex:grammar-used",
    "language-tex:grammar-used",
    "language-latexsimple:grammar-used"
  ],
  "repository": "https://github.com/James-Yu/Atom-LaTeX",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    }
  },
  "dependencies": {
    "ws": "^1.1.1",
    "hasbin": "^1.2.3",
    "open": "^0.0.5",
    "atom-message-panel": "^1.3.0",
    "atom-space-pen-views": "^2.2.0"
  }
}
