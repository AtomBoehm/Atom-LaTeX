'.text.tex.latex':
  'editor':
    'softTabs': true
    'tabLength': 2
    'softWrap': true
    'commentStart': '% '
  'autocomplete-plus':
    'extraWordCharacters': '-:_/'
  'bracket-matcher':
    'autocompleteCharacters': [
      '$$',
      '``',
      '{}',
      '()',
      '[]',
      '""',
      '\'\''
    ]
'.text.tex.tex':
  'editor':
    'commentStart': '% '
